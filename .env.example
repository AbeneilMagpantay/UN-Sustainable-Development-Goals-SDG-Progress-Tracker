# Environment variables for SDG Progress Tracker
# Copy this file to .env and fill in your values

# Google Cloud
GOOGLE_APPLICATION_CREDENTIALS=/path/to/your/service-account-key.json
GCP_PROJECT_ID=sdg-tracker

# BigQuery
BQ_DATASET_RAW=raw
BQ_DATASET_STAGING=staging
BQ_DATASET_MARTS=marts

# Optional: API Keys (if needed for rate limiting bypass)
# WORLD_BANK_API_KEY=your_key_here
